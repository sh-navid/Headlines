{
    "manifest_version": 2,
    "name": "My TicTocToe Addon",
    "version": "1.0",
    "description": "This is a simple tic-toc-toe game",
    "icons": {
        "48": "icon.png"
    },
    "browser_action": {
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "matches": [
                "*://localhost/*"
            ],
            "js": [
                "source.js"
            ]
        }
    ]
}